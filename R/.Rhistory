mutate(permno = PERMNO) %>%
dplyr::select(-PERMNO)
# It seems that datashare with RET has more rows than the datashare file
# Full join them for now
datashare_RAW_join <- full_join(datashare_RAW, datashare_RET_RAW, by = c("permno", "DATE")) %>%
# Reorder variables
mutate(stock = permno) %>%
dplyr::select(-permno) %>%
